{
  "name": "social-distance",
  "version": "0.1.0",
  "description": "Anti-clutter fitness leaderboard",
  "main": "dist/app.js",
  "scripts": {
    "build": "tsc",
    "services:up": "docker-compose up -d",
    "services:stop": "docker-compose stop",
    "dev": "npm run services:up && ts-node-dev --respawn --transpile-only src/app.ts"
  },
  "dependencies": {
    "express": "^4.18.2",
    "ioredis": "^5.3.2",
    "pg": "^8.11.3"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/node": "^20.10.5",
    "@types/pg": "^8.10.9",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.3.3"
  }
}